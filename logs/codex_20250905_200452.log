		method: 'POST',
		headers: { 'content-type': 'application/json' },
		body: JSON.stringify({ contents: [{ parts: [{ text: prompt }] }] })
	});
	if (!res.ok) throw new Error(`Gemini error ${res.status}: ${await res.text()}`);
